{
  "cli": {
    "version": ">= 16.23.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" }
    },
    "preview": {
      "distribution": "internal",
      "prebuildCommand": "bash ./scripts/generate-google-services.sh",
      "android": { "buildType": "apk" }
    },
    "production": {
      "autoIncrement": true,
      "prebuildCommand": "bash ./scripts/generate-google-services.sh",
      "android": { "buildType": "apk" }
    }
  },
  "submit": {
    "production": {}
  }
}
